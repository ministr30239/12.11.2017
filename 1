Напишите программу, которая удаляет из массива все простые числа встречающиеся больше одного раза.

#include <iostream>
￼  
￼ using namespace std;
￼ bool divisibility(int a){
￼    if( a == 2) return true;
￼     if( (a==0) || (a%2 == 0) ) return false;
￼     if ( a == 1 ) return false;
￼     int k = a / 2;
￼     for( long i = 3; i <= k; i++)
￼        if( a % i == 0) return false;
￼           return true;
￼ }
